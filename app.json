{
  "expo": {
    "name": "MediLineApp",
    "slug": "MediLineApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medilineapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hisharasamaranayake.MediLineApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#00A1A1",
      "ios": {
        "sounds": [
          {
            "name": "tone1.wav",
            "critical": false
          },
          {
            "name": "tone2.wav",
            "critical": false
          },
          {
            "name": "tone3.wav",
            "critical": false
          }
        ]
      },
      "android": {
        "sounds": [
          {
            "name": "tone1",
            "critical": false
          },
          {
            "name": "tone2",
            "critical": false
          },
          {
            "name": "tone3",
            "critical": false
          }
        ]
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59c100e0-5f93-4b3f-8761-84ca5e9911cd"
      }
    }
  }
}
